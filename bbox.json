{
	"id": "com.demo.gauge2",
	"version": "2.0.0",
	"name": "Demo Gauge2",
	"description": "A gauge demo",
	"newInstancePrefix": "Gauge",
	"icon": "https://omiklakhani1.github.io/Custom_widget.github.io/icon.png",
	"vendor": "OMIK",
	"eula": "EULA",
	"license": "1.0",
	"webcomponents": [
		{
			"kind": "main",
			"tag": "com-demo-gauge2",
			"url": "https://omiklakhani1.github.io/Custom_widget.github.io/box2.js",
			"integrity": "",
			"ignoreIntegrity": true
		},
		{
			"kind": "builder",
			"tag": "com-demo-box-bps2",
			"url": "https://omiklakhani1.github.io/Custom_widget.github.io/Bps.js",
			"integrity": "",
			"ignoreIntegrity": true
		}
	],
	"properties": {
		"value": {
			"type": "number",
			"description": "Gauge value",
			"default": 0
		},
		"info": {
			"type": "string",
			"description": "Gauge info",
			"default": ""
		},
		"color": {
			"type": "string",
			"description": "Text Color info",
			"default": "#3498db"
		},
		"width": {
			"type": "integer",
			"default": 100
		},
		"height": {
			"type": "integer",
			"default": 100
		}
	},"methods":
	{
	
			"setValue": 
			{
				"description": "Sets the Gauge value.",
				"parameters": [
	
					{
						"name": "newValue",
						"type": "number",
						"description": "Gauge value"
						
					}
				
						  ],
	
				"body": "this.value = newValue;"
			},
	
			"setinfo": 
			{
				"description": "Sets the Gauge info.",
				"parameters": [
					{
						"name": "newInfo",
						"type": "string",
						"description": "Gauge info"
					}
	
						  ],
	
				"body": "this.info = newInfo;"
			},
	
	
			"setColor": 
			{
				"description": "Sets the Gauge Color.",
				"parameters": [
					{
						"name": "newColor",
						"type": "string",
						"description": "Text Color info"
					}
						  ],
	
				"body": "this.color = newColor;"
			},
	
			
			"getValue": {
				"returnType": "number",
				"description": "Returns the Gauge value.",
				"body": "return this.value;"
						},
			
			"getinfo": {
				"returnType": "number",
				"description": "Returns the Gauge value.",
				"body": "return this.value;"
					},
			
			"getColor": {
				"returnType": "number",
				"description": "Returns the Gauge value.",
				"body": "return this.value;"
					}
	},
		
	"events": {
		"onClick": {
			"description": "Called when the user clicks the Box."
		}
	}
}

 